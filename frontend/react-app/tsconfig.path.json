{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@src": ["."],
      "@src/*": ["./*"],
      "@api": ["api"],
      "@api/*": ["api/*"],
      "@assets/*": ["assets/*"],
      "@components/*": ["components/*"],
      "@hooks/*": ["hooks/*"],
      "@mocks/*": ["mocks/*"],
      "@pages": ["pages"],
      "@pages/*": ["pages/*"],
      "@router": ["router"],
      "@router/*": ["router/*"],
      "@recoil/*": ["recoil/*"],
      "@type": ["types"],
      "@type/*": ["types/*"],
      "@utils/*": ["utils/*"]
    }
  }
}
